@model LoLApiChallenge.Models.FactionDetailsModel

<h3>@Model.Name</h3>
@foreach (var champ in Model.Champs)
{
    <div class="faction-stats-wrap col-lg-4">
        <img src="@champ.ImageUrl" width="70" height="70" />
        <h6 class="white" style="white-space: nowrap;">@champ.Name</h6>
    </div>
}
