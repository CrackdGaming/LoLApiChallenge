@model LoLApiChallenge.Models.FactionData


<div class="faction" data-id="@Model.Name">
    <img src="@Url.Action("GetImage", "Home", new { name = Model.Name })" />
    <div>@Model.Name</div>
    <div>Score: @Model.Kda</div>
    <div>Average KDA</div>
    <div>@Model.Kills.ToString("N0") / @Model.Deaths.ToString("N0") / @Model.Assists.ToString("N0")</div>
</div>